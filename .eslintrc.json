{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins":[
        "jsx-a11y",
        "prettier"
    ],
    "rules": {
        "linebreak-style": "off",
        "prettier/prettier": "off"
    }
}
